{
    "name": "JS Cube",
    "version": "1.0",
    "description": "Inject your own JS into specific websites!",
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "compat.js",
            "common.js",
            "background.js"
        ]
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true,
        "browser_style": false,
        "chrome_style": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/ic_launcher_js72.png"
    },
    "icons": {
        "48": "images/ic_launcher_js48.png",
        "72": "images/ic_launcher_js72.png",
        "144": "images/ic_launcher_js144.png"
    },
    "manifest_version": 2,
    "browser_specific_settings": {
        "gecko": {
            "id": "{9805bd00-d0a7-4e6b-9267-39053437b622}",
            "strict_min_version": "57.0"
        }
    }
}