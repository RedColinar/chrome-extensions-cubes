{
    "name": "JS Cube",
    "version": "1.0",
    "description": "Inject your own JS into specific website!",
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "logic.js",
            "background.js"
        ],
        "persistent": false
    },
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/get_started48.png"
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "manifest_version": 2
}